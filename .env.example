# Supabase設定
# Supabaseプロジェクトの公開URL（ダッシュボードで確認可能）
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
# Supabase匿名キー（クライアントサイドで安全に使用可能）
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.example-key

# Mapbox設定
# Mapbox GL JS V3用のアクセストークン
NEXT_PUBLIC_MAPBOX_TOKEN=pk.example.mapbox-token-goes-here

# PWA設定
# アプリケーション名（マニフェストとメタデータに使用）
NEXT_PUBLIC_APP_NAME=Sonory